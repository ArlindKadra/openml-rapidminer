              <operator activated="true" class="multiply" compatibility="5.3.000" expanded="true" height="94" name="Multiply" width="90" x="246" y="120"/>
              <operator activated="true" class="join" compatibility="5.3.000" expanded="true" height="76" name="trainingSet" width="90" x="447" y="30">
