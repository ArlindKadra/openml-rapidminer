  <operator activated="true" class="process" compatibility="5.3.000" expanded="true" name="Process">
    <description>&lt;h1&gt;&lt;b&gt;OpenML Cross validation Task&lt;/b&gt; &lt;/h1&gt;
This is a template process for performing OpenML cross validation task. The configuration of examples used in each fold are provided by the server and are already configured in the process.
&lt;h2&gt;Tips for editing&lt;/h2&gt;
&lt;ol&gt;
&lt;li&gt; The learner (Naive Bayes by default) could be replaced inside the &lt;b&gt;Loop Configs&lt;/b&gt; subprocess.&lt;/li&gt;
&lt;li&gt; Any preprocessing step could be added before the &lt;b&gt;Loop Configs&lt;/b&gt; subprocess, that will ensure that the preprocessing is applied on both
test and training data &lt;/li&gt;
&lt;/ol&gt;</description>
    <parameter key="logverbosity" value="init"/>
    <parameter key="random_seed" value="2001"/>
    <parameter key="send_mail" value="never"/>
    <parameter key="notification_email" value=""/>
    <parameter key="process_duration_for_mail" value="30"/>
    <parameter key="encoding" value="SYSTEM"/>
    <process expanded="true" height="611" width="1083">
      <operator activated="true" class="retrieve" compatibility="5.3.000" expanded="true" height="60" name="Retrieve Data" width="90" x="45" y="75">
        <description>Retrives the dataset for the OpenML task
</description>
        <parameter key="repository_entry" value="$DATA_PATH$"/>
      </operator>
      <operator activated="true" class="loop_repository" compatibility="5.3.000" expanded="true" height="76" name="Loop Configs" width="90" x="179" y="120">
        <parameter key="repository_folder" value="$TRAIN_PATH$"/>
        <parameter key="entry_type" value="IOObject"/>
        <parameter key="filter" value="fold.*"/>
        <parameter key="filtered_string" value="file name (last part of the path)"/>
        <parameter key="entry_name_macro" value="entry_name"/>
        <parameter key="repository_path_macro" value="repository_path"/>
        <parameter key="parent_folder_macro" value="parent_folder"/>
        <parameter key="recursive" value="false"/>
        <process expanded="true" height="611" width="1083">
          <operator activated="true" class="generate_macro" compatibility="5.3.000" expanded="true" height="76" name="Generate Macro" width="90" x="112" y="30">
            <description>can also use other macro values like belowreplace("%{repository_path}","train","test")</description>
            <list key="function_descriptions">
              <parameter key="test_path" value="replace(&quot;%{repository_path}&quot;,&quot;train&quot;,&quot;test&quot;)"/>
            </list>
            <parameter key="use_standard_constants" value="true"/>
          </operator>
          <operator activated="true" class="multiply" compatibility="5.3.000" expanded="true" height="94" name="Multiply" width="90" x="112" y="210"/>
          <operator activated="true" class="join" compatibility="5.3.000" expanded="true" height="76" name="trainingSet" width="90" x="313" y="165">
            <parameter key="remove_double_attributes" value="true"/>
            <parameter key="join_type" value="inner"/>
            <parameter key="use_id_attribute_as_key" value="true"/>
            <list key="key_attributes"/>
            <parameter key="keep_both_join_attributes" value="false"/>
          </operator>
          <operator activated="true" class="set_role" compatibility="5.3.000" expanded="true" height="76" name="Set Label" width="90" x="447" y="75">
            <parameter key="name" value="$TARGET_FEATURE$"/>
            <parameter key="target_role" value="label"/>
            <list key="set_additional_roles"/>
          </operator>
          <operator activated="true" class="naive_bayes" compatibility="5.3.000" expanded="true" height="76" name="Naive Bayes" width="90" x="581" y="30">
            <parameter key="laplace_correction" value="true"/>
          </operator>
          <operator activated="true" class="retrieve" compatibility="5.3.000" expanded="true" height="60" name="testset Config" width="90" x="179" y="390">
            <parameter key="repository_entry" value="%{test_path}"/>
          </operator>
          <operator activated="true" class="join" compatibility="5.3.000" expanded="true" height="76" name="testSet" width="90" x="447" y="390">
            <parameter key="remove_double_attributes" value="true"/>
            <parameter key="join_type" value="inner"/>
            <parameter key="use_id_attribute_as_key" value="true"/>
            <list key="key_attributes"/>
            <parameter key="keep_both_join_attributes" value="false"/>
          </operator>
          <operator activated="true" class="apply_model" compatibility="5.3.000" expanded="true" height="76" name="Apply Model" width="90" x="715" y="120">
            <list key="application_parameters"/>
            <parameter key="create_view" value="false"/>
          </operator>
          <operator activated="true" class="select_attributes" compatibility="5.3.000" expanded="true" height="76" name="Select prediction" width="90" x="715" y="300">
            <parameter key="attribute_filter_type" value="regular_expression"/>
            <parameter key="attribute" value=""/>
            <parameter key="attributes" value=""/>
            <parameter key="regular_expression" value="repeat|fold|id|rowid|prediction.*|confidence.*"/>
            <parameter key="use_except_expression" value="false"/>
            <parameter key="value_type" value="attribute_value"/>
            <parameter key="use_value_type_exception" value="false"/>
            <parameter key="except_value_type" value="time"/>
            <parameter key="block_type" value="attribute_block"/>
            <parameter key="use_block_type_exception" value="false"/>
            <parameter key="except_block_type" value="value_matrix_row_start"/>
            <parameter key="invert_selection" value="false"/>
            <parameter key="include_special_attributes" value="true"/>
          </operator>
          <operator activated="true" class="rename" compatibility="5.3.000" expanded="true" height="76" name="Rename id" width="90" x="849" y="300">
            <parameter key="old_name" value="id"/>
            <parameter key="new_name" value="rowid"/>
            <list key="rename_additional_attributes"/>
          </operator>
          <operator activated="true" class="set_role" compatibility="5.3.000" expanded="true" height="76" name="Set rowid" width="90" x="916" y="165">
            <parameter key="name" value="rowid"/>
            <parameter key="target_role" value="rowid"/>
            <list key="set_additional_roles">
              <parameter key="rowid" value="rowid"/>
            </list>
          </operator>
          <connect from_port="repository object" to_op="Generate Macro" to_port="through 1"/>
          <connect from_port="in 1" to_op="Multiply" to_port="input"/>
          <connect from_op="Generate Macro" from_port="through 1" to_op="trainingSet" to_port="left"/>
          <connect from_op="Multiply" from_port="output 1" to_op="trainingSet" to_port="right"/>
          <connect from_op="Multiply" from_port="output 2" to_op="testSet" to_port="left"/>
          <connect from_op="trainingSet" from_port="join" to_op="Set Label" to_port="example set input"/>
          <connect from_op="Set Label" from_port="example set output" to_op="Naive Bayes" to_port="training set"/>
          <connect from_op="Naive Bayes" from_port="model" to_op="Apply Model" to_port="model"/>
          <connect from_op="testset Config" from_port="output" to_op="testSet" to_port="right"/>
          <connect from_op="testSet" from_port="join" to_op="Apply Model" to_port="unlabelled data"/>
          <connect from_op="Apply Model" from_port="labelled data" to_op="Select prediction" to_port="example set input"/>
          <connect from_op="Select prediction" from_port="example set output" to_op="Rename id" to_port="example set input"/>
          <connect from_op="Rename id" from_port="example set output" to_op="Set rowid" to_port="example set input"/>
          <connect from_op="Set rowid" from_port="example set output" to_port="out 1"/>
          <portSpacing port="source_repository object" spacing="0"/>
          <portSpacing port="source_in 1" spacing="0"/>
          <portSpacing port="source_in 2" spacing="0"/>
          <portSpacing port="sink_out 1" spacing="0"/>
          <portSpacing port="sink_out 2" spacing="0"/>
        </process>
      </operator>
      <operator activated="true" class="append" compatibility="5.3.000" expanded="true" height="76" name="Append results" width="90" x="313" y="120">
        <parameter key="datamanagement" value="double_array"/>
        <parameter key="merge_type" value="all"/>
      </operator>
      <operator activated="true" class="generate_attributes" compatibility="5.3.000" expanded="true" height="76" name="Generate Attributes" width="90" x="447" y="120">
        <list key="function_descriptions">
          <parameter key="newid" value="rowid-1"/>
        </list>
        <parameter key="use_standard_constants" value="true"/>
        <parameter key="keep_all" value="true"/>
      </operator>
      <operator activated="true" class="select_attributes" compatibility="5.3.000" expanded="true" height="76" name="Select Attributes (2)" width="90" x="581" y="120">
        <parameter key="attribute_filter_type" value="single"/>
        <parameter key="attribute" value="rowid"/>
        <parameter key="attributes" value=""/>
        <parameter key="use_except_expression" value="false"/>
        <parameter key="value_type" value="attribute_value"/>
        <parameter key="use_value_type_exception" value="false"/>
        <parameter key="except_value_type" value="time"/>
        <parameter key="block_type" value="attribute_block"/>
        <parameter key="use_block_type_exception" value="false"/>
        <parameter key="except_block_type" value="value_matrix_row_start"/>
        <parameter key="invert_selection" value="true"/>
        <parameter key="include_special_attributes" value="true"/>
      </operator>
      <operator activated="true" class="set_role" compatibility="5.3.000" expanded="true" height="76" name="Set Role (3)" width="90" x="715" y="120">
        <parameter key="name" value="newid"/>
        <parameter key="target_role" value="id"/>
        <list key="set_additional_roles"/>
      </operator>
      <operator activated="true" class="rename" compatibility="5.3.000" expanded="true" height="76" name="Rename (2)" width="90" x="849" y="120">
        <parameter key="old_name" value="newid"/>
        <parameter key="new_name" value="rowid"/>
        <list key="rename_additional_attributes"/>
      </operator>
      <operator activated="true" class="store" compatibility="5.3.000" expanded="true" height="60" name="Store" width="90" x="983" y="120">
        <parameter key="repository_entry" value="$RESULT_PATH$"/>
      </operator>
      <connect from_op="Retrieve Data" from_port="output" to_op="Loop Configs" to_port="in 1"/>
      <connect from_op="Loop Configs" from_port="out 1" to_op="Append results" to_port="example set 1"/>
      <connect from_op="Append results" from_port="merged set" to_op="Generate Attributes" to_port="example set input"/>
      <connect from_op="Generate Attributes" from_port="example set output" to_op="Select Attributes (2)" to_port="example set input"/>
      <connect from_op="Select Attributes (2)" from_port="example set output" to_op="Set Role (3)" to_port="example set input"/>
      <connect from_op="Set Role (3)" from_port="example set output" to_op="Rename (2)" to_port="example set input"/>
      <connect from_op="Rename (2)" from_port="example set output" to_op="Store" to_port="input"/>
      <connect from_op="Store" from_port="through" to_port="result 1"/>
      <portSpacing port="source_input 1" spacing="0"/>
      <portSpacing port="sink_result 1" spacing="0"/>
      <portSpacing port="sink_result 2" spacing="0"/>
    </process>
  </operator>
</process>
